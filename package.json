  "name": "tinkerweb",
  "version": "1.0.1",
  "description": "Follow the Instructions below to build & run Tinker Web in your local environment.",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
